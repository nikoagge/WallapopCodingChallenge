{"primaryContentSections":[{"kind":"declarations","declarations":[{"tokens":[{"kind":"keyword","text":"func"},{"kind":"text","text":" "},{"kind":"identifier","text":"sentMessage"},{"kind":"text","text":"("},{"kind":"externalParam","text":"_"},{"kind":"text","text":" "},{"kind":"internalParam","text":"selector"},{"kind":"text","text":": "},{"kind":"typeIdentifier","text":"Selector","preciseIdentifier":"s:10ObjectiveC8SelectorV"},{"kind":"text","text":") -> "},{"kind":"typeIdentifier","text":"Observable","preciseIdentifier":"s:7RxSwift10ObservableC"},{"kind":"text","text":"<["},{"kind":"keyword","text":"Any"},{"kind":"text","text":"]>"}],"languages":["swift"],"platforms":["iOS"]}]},{"kind":"content","content":[{"anchor":"return-value","level":2,"type":"heading","text":"Return Value"},{"type":"paragraph","inlineContent":[{"type":"text","text":"Observable sequence of arguments passed to "},{"type":"codeVoice","code":"selector"},{"type":"text","text":" method."}]}]},{"kind":"parameters","parameters":[{"name":"selector","content":[{"type":"paragraph","inlineContent":[{"type":"text","text":"Selector used to filter observed invocations of delegate methods."}]}]}]},{"kind":"content","content":[{"anchor":"discussion","level":2,"type":"heading","text":"Discussion"},{"type":"paragraph","inlineContent":[{"type":"text","text":"Only methods that have "},{"type":"codeVoice","code":"void"},{"type":"text","text":" return value can be observed using this method because"},{"type":"text","text":" "},{"type":"text","text":"those methods are used as a notification mechanism. It doesn’t matter if they are optional"},{"type":"text","text":" "},{"type":"text","text":"or not. Observing is performed by installing a hidden associated "},{"type":"codeVoice","code":"PublishSubject"},{"type":"text","text":" that is"},{"type":"text","text":" "},{"type":"text","text":"used to dispatch messages to observers."}]},{"type":"paragraph","inlineContent":[{"type":"text","text":"Delegate methods that have non "},{"type":"codeVoice","code":"void"},{"type":"text","text":" return value can’t be observed directly using this method"},{"type":"text","text":" "},{"type":"text","text":"because:"}]},{"type":"unorderedList","items":[{"content":[{"type":"paragraph","inlineContent":[{"type":"text","text":"those methods are not intended to be used as a notification mechanism, but as a behavior customization mechanism"}]}]},{"content":[{"type":"paragraph","inlineContent":[{"type":"text","text":"there is no sensible automatic way to determine a default return value"}]}]}]},{"type":"paragraph","inlineContent":[{"type":"text","text":"In case observing of delegate methods that have return type is required, it can be done by"},{"type":"text","text":" "},{"type":"text","text":"manually installing a "},{"type":"codeVoice","code":"PublishSubject"},{"type":"text","text":" or "},{"type":"codeVoice","code":"BehaviorSubject"},{"type":"text","text":" and implementing delegate method."}]},{"type":"paragraph","inlineContent":[{"type":"text","text":"e.g."}]},{"type":"codeListing","syntax":null,"code":["\/\/ delegate proxy part (RxScrollViewDelegateProxy)","","let internalSubject = PublishSubject<CGPoint>","","public func requiredDelegateMethod(scrollView: UIScrollView, arg1: CGPoint) -> Bool {","    internalSubject.on(.next(arg1))","    return self._forwardToDelegate?.requiredDelegateMethod?(scrollView, arg1: arg1) ?? defaultReturnValue","}"]},{"type":"paragraph","inlineContent":[{"type":"text","text":"…."}]},{"type":"codeListing","syntax":null,"code":["\/\/ reactive property implementation in a real class (`UIScrollView`)","public var property: Observable<CGPoint> {","    let proxy = RxScrollViewDelegateProxy.proxy(for: base)","    return proxy.internalSubject.asObservable()","}"]},{"type":"paragraph","inlineContent":[{"type":"strong","inlineContent":[{"type":"text","text":"In case calling this method prints “Delegate proxy is already implementing "},{"type":"codeVoice","code":"\\(selector)"},{"type":"text","text":","},{"type":"text","text":" "},{"type":"text","text":"a more performant way of registering might exist.”, that means that manual observing method"},{"type":"text","text":" "},{"type":"text","text":"is required analog to the example above because delegate method has already been implemented."}]}]}]}],"schemaVersion":{"major":0,"minor":2,"patch":0},"sections":[],"variants":[{"paths":["\/documentation\/rxcocoa\/delegateproxy\/sentmessage(_:)"],"traits":[{"interfaceLanguage":"swift"}]}],"identifier":{"url":"doc:\/\/org.cocoapods.RxCocoa\/documentation\/RxCocoa\/DelegateProxy\/sentMessage(_:)","interfaceLanguage":"swift"},"abstract":[{"type":"text","text":"Returns observable sequence of invocations of delegate methods. Elements are sent "},{"type":"emphasis","inlineContent":[{"type":"text","text":"before method is invoked"}]},{"type":"text","text":"."}],"kind":"symbol","metadata":{"fragments":[{"kind":"keyword","text":"func"},{"kind":"text","text":" "},{"kind":"identifier","text":"sentMessage"},{"kind":"text","text":"("},{"kind":"typeIdentifier","text":"Selector","preciseIdentifier":"s:10ObjectiveC8SelectorV"},{"kind":"text","text":") -> "},{"kind":"typeIdentifier","text":"Observable","preciseIdentifier":"s:7RxSwift10ObservableC"},{"kind":"text","text":"<["},{"kind":"keyword","text":"Any"},{"kind":"text","text":"]>"}],"title":"sentMessage(_:)","roleHeading":"Instance Method","role":"symbol","symbolKind":"method","externalID":"s:7RxCocoa13DelegateProxyC11sentMessagey0A5Swift10ObservableCySayypGG10ObjectiveC8SelectorVF","modules":[{"name":"RxCocoa"}]},"hierarchy":{"paths":[["doc:\/\/org.cocoapods.RxCocoa\/documentation\/RxCocoa","doc:\/\/org.cocoapods.RxCocoa\/documentation\/RxCocoa\/DelegateProxy"]]},"references":{"doc://org.cocoapods.RxCocoa/documentation/RxCocoa":{"role":"collection","title":"RxCocoa","abstract":[],"identifier":"doc:\/\/org.cocoapods.RxCocoa\/documentation\/RxCocoa","kind":"symbol","type":"topic","url":"\/documentation\/rxcocoa"},"doc